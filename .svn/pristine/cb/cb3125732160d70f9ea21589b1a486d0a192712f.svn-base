@charset "utf-8";
/* CSS Document */

/* Native elements */
body {	background-color:#EFF1F1;	}

.header 	{	margin: 0 auto;	}
.footer 	{	padding-bottom: 20px;	padding-top: 10px;	}
.nav-bar 	{	width: 1000px;	margin: 0 auto;	font-size: 10px;	}
.container 	{	/*	min-height: 400px; */	}
.inner-container { /*	margin-top: 30px;*/	}
.outer-container {  width:1000px; margin:0 auto; background-color:#ffffff; padding-bottom: 40px;  margin-top: -20px !important; }
.container_shadow_top{-moz-border-radius:0px 0px 6px 6px;border-radius:0px 0px 6px 6px;-webkit-border-radius:6px;-webkit-border-top-right-radius:0px;-webkit-border-top-left-radius:0px;-moz-box-shadow:0 0px 3px rgba(0,0,0,0.298);-webkit-box-shadow:0 0px 3px rgba(0,0,0,0.298);box-shadow:0 0px 3px rgba(0,0,0,0.298);background-color:#FFFFFF;width:1000px;margin:0px auto;}

/* border */
.br1 { border: 1px solid #ccc; }

/* box */
.box-preset-1 { padding-left: 70px; padding-top: 18px; padding-bottom: 18px; border: 1px solid #ccc; }
.box-preset-2 {	background-color:#FEFFD8; border: 1px solid #FFE438; padding:10px; }

/* icons-32 */
