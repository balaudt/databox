<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sagarius.goddess.client.view.util">
	<g:HTMLPanel ui:field="mainPanel">
		<g:FlowPanel>
			<g:HTML ui:field="title" styleName="fl"></g:HTML>
		</g:FlowPanel>
		<g:FlowPanel ui:field="header" styleName="clear"></g:FlowPanel>
		<g:DeckPanel ui:field="contentDecker">
			<g:FlowPanel styleName="clear fl ">
				<g:Image url="/goddess/images/gadget-load.gif" styleName="image clear"></g:Image>
			</g:FlowPanel>
			<g:FlowPanel ui:field="contentsPanel" styleName="fl">
			</g:FlowPanel>
		</g:DeckPanel>
		<s:ControlPanel ui:field="controlsPanel" styleName="clear"></s:ControlPanel>
		<g:FlowPanel ui:field="footer" styleName="clear"></g:FlowPanel>
		<g:FlowPanel ui:field="statusPanel" styleName="fl clear mt10">
			<g:HTML ui:field="statusMessage" styleName="h12"></g:HTML>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 